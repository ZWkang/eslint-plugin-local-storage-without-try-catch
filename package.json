{
  "name": "eslint-plugin-local-storage-with-try-catch",
  "version": "0.0.1-beta.1",
  "packageManager": "pnpm@7.9.0",
  "description": "> description:",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "zwkang",
    "email": "kang95630@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zwkang/eslint-plugin-local-storage-without-try-catch.git"
  },
  "bugs": "https://github.com/zwkang/eslint-plugin-local-storage-without-try-catch/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "lint": "eslint --cache .",
    "test": "tsx ./test/index.test.ts",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "release": "npm publish",
    "bump:version": "bumpp",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "bumpp": "^9.2.0",
    "eslint-config-alloy": "^5.1.2",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6",
    "zwkang-prettier-config": "^0.0.3",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.25",
    "rslog": "^1.2.1"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
